{
  "asi": false,
  "bitwise": true,
  "browser": true,
  "boss": false,
  "curly": true,
  "debug": false,
  "eqeqeq": true,
  "eqnull": false,
  "es3": true,
  "esnext": false,
  "evil": false,
  "expr": false,
  "forin": true,
  "funcscope": false,
  "immed": true,
  "indent": 2,
  "iterator": false,
  "jquery": false,
  "lastsemic": false,
  "latedef": true,
  "laxbreak": false,
  "laxcomma": false,
  "loopfunc": false,
  "maxcomplexity": 5,
  //"maxlen": 85,
  //"maxstatements": 20, // tests have lots of statements
  "multistr": false,
  "newcap": false,  // moment() starts with a lowercase letter
  "noarg": true,
  "node": false,
  "noempty": true,
  "nonew": true,
  "proto": false,
  //"quotmark": true,
  "scripturl": false,
  "shadow": false,
  "smarttabs": false,
  "strict": true,
  "sub": false,
  "supernew": false,
  "trailing": true,
  "undef": true,
  "unused": true,
  "globals": {
    "define": false,
    "require": false,
    // Jasmine-specific. Should probably move to a test-only config
    "it": false,
    "describe": false,
    "spyOn": false,
    "expect": false,
    "beforeEach": false,
    "afterEach": false,
    "runs": false,
    "waits": false,
    "waitsFor": false,
    "setFixtures": false,
    "appendSetFixtures": false,
    "jasmine": false
  }
}
